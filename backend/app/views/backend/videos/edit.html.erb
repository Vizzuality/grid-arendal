<div class="l-items-index">
  <%= render 'backend/shared/media_contents/media_contents', key: "videos", items: @videos %>
</div>

<div class="l-items-detail">
  <%= simple_form_for @video, html: { class: 'c-form' } do |f| %>
    <%= render 'form',
        form: f,
        tags: @tags,
        activities: @activities,
        publications: @publications,
        news_articles: @news_articles
    %>
  <% end %>
</div>
