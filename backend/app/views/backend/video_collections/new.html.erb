<div class="l-items-index">
  <%= render 'backend/shared/media_contents/media_contents', key: "video_collections", items: @video_collections %>
</div>

<div class="l-items-detail">
  <%= simple_form_for @video_collection, html: { class: 'c-form' } do |f| %>
    <%= render 'form',
      form: f,
      collection_videos: @collection_videos,
      tags: @tags
    %>
  <% end %>
</div>
